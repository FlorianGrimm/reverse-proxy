{
  "Comment": "The back end - inner front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5003"
      }
    }
  },
  "ReverseProxy": {
    "Tunnels": {
      "tunnel5001alpha": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "alpha",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "ClientCertificate",
          "ClientCertificate": {
            "Path": "localhostclient1.pfx",
            "Password": "testPassword1"
          }
        }
      }
    },
    "Routes": {
      "routeBEroot": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Order": 101
      },
      "routeBEalpha": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "/alpha/{**catch-all}"
        },
        "Order": 100
      },
      "routeBEgamma": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "gamma": {
        "Destinations": {
          "gamma/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "LocalFile": {
      "LogLevel": {
        "Default": "Trace"
      },
      "FileName": "server3_"
    }
  }
}
