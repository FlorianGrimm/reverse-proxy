{
  "Comment": "The tunnel front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route": {
        "ClusterId": "tunnel",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "tunnel": {
        "Transport": "TunnelHTTP2",
        "TransportAuthentication": {
          "Mode": "Basic"
        }
      }
    },
    "AuthenticationBasic": {
      "Password": "testPassword"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LocalFile": {
      "BaseDirectory": "%HOME%",
      "Directory": "LogFiles\\Application",
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  }
}
