{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LocalFile": {
      "BaseDirectory": "%HOME%",
      "Directory": "LogFiles\\Application",
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy-Backend": {
    "Tunnels": {
      "tunnel": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "tunnel",
        "Transport": "TunnelHTTP2",
        "TransportAuthentication": {
          "Mode": "Basic"
        }
      }
    },
    "Routes": {
      "route": {
        "ClusterId": "tunnel",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "tunnel": {
        "Transport": "TunnelHTTP2",
        "TransportAuthentication": {
          "Mode": "Basic"
        }
      }
    },
    "AuthenticationBasic": {
      "Password": "testPassword"
    }
  },
  "ReverseProxy-Frontend": {
    "Routes": {
      "route": {
        "ClusterId": "tunnel",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "tunnel": {
        "Transport": "TunnelHTTP2",
        "TransportAuthentication": {
          "Mode": "Basic"
        }
      }
    },
    "AuthenticationBasic": {
      "Password": "testPassword"
    }
  }
}
