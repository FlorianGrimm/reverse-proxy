{
  "Comment": "The back end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5003"
      }
    }
  },
  "ReverseProxy": {
    "Tunnels": {
      "tunnel": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "tunnel",
        "Transport": "TunnelHTTP2",
        "TransportAuthentication": {
          "Mode": "Basic"
        }
      }
    },
    "Routes": {
      "routeBEroot": {
        "ClusterId": "tunnel",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Order": 1
      }
    },
    "Clusters": {
      "tunnel": {
        "Destinations": {
          "local": {
            "Address": "https://localhost:5005"
          }
        }
      }
    },
    "AuthenticationBasic": {
      "Password": "testPassword"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LocalFile": {
      "BaseDirectory": "%HOME%",
      "Directory": "LogFiles\\Application",
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  }
}
