{
  "Comment": "The back end - inner front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5003"
      }
    }
  },
  "ReverseProxy": {
    "Tunnels": {
      "tunnelalphaFE1": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "alpha",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "JwtBearer"
        }
      },
      "tunnelbetaFE1": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "beta",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "JwtBearer"
        }
      }
    },
    "Routes": {
      "routeBEroot": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Order": 101
      },
      "routeBEalpha": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "/alpha/{**catch-all}"
        },
        "Order": 100
      },
      "routeBEbeta": {
        "ClusterId": "beta",
        "Match": {
          "Path": "/beta/{**catch-all}"
        },
        "Order": 100
      },
      "routeBEgamma": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "beta": {
        "Destinations": {
          "beta": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "gamma": {
        "Destinations": {
          "gamma/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  },
  "Logging-Warning": {
    "LogLevel": {
      "Default": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Logging-Information": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Information",
        "Microsoft.AspNetCore.Server.Kestrel.Http2": "Warning",
        "Microsoft.AspNetCore.Server.Kestrel.Transport.Sockets": "Warning",
        "Yarp": "Information"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "LocalFile": {
      "LogLevel": {
        "Default": "Trace"
      },
      "FileName": "server3_"
    }
  }
}
