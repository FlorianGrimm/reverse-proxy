{
  "Urls": "https://localhost:5001/",
  "Comment": "The tunnel front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ReverseProxy": {
    "CertificateConfig": {
      "CertificateRoot": "%Assembly%"
    },
    "Routes": {
      "route0FE1": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route1FE1": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "/alpha/{**catch-all}"
        }
      },
      "route2FE1": {
        "ClusterId": "beta",
        "Match": {
          "Path": "/beta/{**catch-all}"
        }
      },
      "route3FE1": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "ClientCertificate",
          "ClientCertificate": {
            "Path": "localhostclient1.pfx",
            "Password": "testPassword1"
          }
        }
      },
      "beta": {
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "ClientCertificate",
          "ClientCertificate": {
            "Path": "localhostclient1.pfx",
            "Password": "testPassword1"
          }
        }
      },
      "gamma": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://example.com/"
          }
        },
        "Transport": "Forwarder"
      }
    }
  },
  "Logging": {
    "LocalFile": {
      "FileName": "server1_"
    }
  }
}
