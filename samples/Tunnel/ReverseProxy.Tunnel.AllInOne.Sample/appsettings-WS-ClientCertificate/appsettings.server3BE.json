{
  "Urls": "https://localhost:5003/",
  "Comment": "The back end - inner front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5003"
      }
    }
  },
  "ReverseProxy": {
    "CertificateConfig": {
      "CertificateRoot": "%Assembly%"
    },
    "Tunnels": {
      "tunnelalphaFE1": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "alpha",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "ClientCertificate",
          "ClientCertificates": [
            {
              "Path": "localhostclient1.pfx",
              "Password": "testPassword1"
            }
          ]
        }
      },
      "tunnelalphaFE2": {
        "Url": "https://localhost:5002",
        "RemoteTunnelId": "alpha",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "ClientCertificate",
          "ClientCertificates": [
            {
              "Path": "localhostclient1.pfx",
              "Password": "testPassword1"
            }
          ]
        }
      },
      "tunnelbetaFE1": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "beta",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "ClientCertificate",
          "ClientCertificates": [
            {
              "Path": "localhostclient1.pfx",
              "Password": "testPassword1"
            }
          ]
        }
      },
      "tunnelbetaFE2": {
        "Url": "https://localhost:5002",
        "RemoteTunnelId": "beta",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "ClientCertificate",
          "ClientCertificates": [
            {
              "Path": "localhostclient1.pfx",
              "Password": "testPassword1"
            }
          ]
        }
      }
    },
    "Routes": {
      "route0T3": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route1T3": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "/alpha/{**catch-all}"
        }
      },
      "route2T3": {
        "ClusterId": "beta",
        "Match": {
          "Path": "/beta/{**catch-all}"
        }
      },
      "route3T3": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "beta": {
        "Destinations": {
          "beta": {
            "Address": "https://localhost:5006"
          }
        }
      },
      "gamma": {
        "Destinations": {
          "gamma/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  },
  "Logging": {
    "LocalFile": {
      "FileName": "server3_"
    }
  }
}
