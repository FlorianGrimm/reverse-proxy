{
  "Urls": "https://localhost:5004/",
  "Comment": "The back end - inner front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5004"
      }
    }
  },
  "ReverseProxy": {
    "Tunnels": {
      "tunnelalphaFE1": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "alpha",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "Negotiate"
        }
      },
      "tunnelalphaFE2": {
        "Url": "https://localhost:5002",
        "RemoteTunnelId": "alpha",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "Negotiate"
        }
      },
      "tunnelbetaFE1": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "beta",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "Negotiate"
        }
      },
      "tunnelbetaFE2": {
        "Url": "https://localhost:5002",
        "RemoteTunnelId": "beta",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "Negotiate"
        }
      }
    },
    "Routes": {
      "route1T4": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2T4": {
        "ClusterId": "beta",
        "Match": {
          "Path": "/beta/{**catch-all}"
        }
      },
      "route3T4": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "beta": {
        "Destinations": {
          "beta": {
            "Address": "https://localhost:5006"
          }
        }
      },
      "gamma": {
        "Destinations": {
          "gamma/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  },
  "Logging": {
    "LocalFile": {
      "FileName": "server4_"
    }
  }
}
