{
  "Comment": "The tunnel front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ReverseProxy": {
    "Tunnels": {
      "loopback": {
        "Url": "http://loopback",
        "RemoteTunnelId": "loopback",
        "Transport": "TunnelLoopback",
        "TransportAuthentication": {
          "Mode": "Loopback"
        },
        "TunnelAuthenticationScheme": "TransportJwtBearerToken"
      }
    },
    "Routes": {
      "routeFERoot": {
        "ClusterId": "root",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "routeFEUser": {
        "ClusterId": "user",
        "AuthorizationPolicy": "Default",
        "Match": {
          "Path": "/user/{**catch-all}"
        }
      },
      "routeFEExample": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "root": {
        "Transport": "TunnelHTTP2",
        "TransportAuthentication": {
          "Mode": "Basic"
        }
      },
      "user": {
        "Transport": "TunnelHTTP2",
        "TransportAuthentication": {
          "Mode": "Basic"
        }
      },
      "gamma": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://example.com/"
          }
        },
        "Transport": "Forwarder"
      }
    },
    "AuthenticationBasic": {
      "Password": "testPassword"
    },
    "AuthorizationTransport": {
      "SigningKeySecret": "testPassword-IsTooSmall!UseMoreText!elf!",
      "Algorithm": "http://www.w3.org/2001/04/xmldsig-more#hmac-sha256",
      "IncludeClaimType": [ "ALL" ],
      "EnableForAllCluster": true,
      "RemoveHeaderAuthenticate": true,
      "Scheme": "",
      "Issuer": "Issuer",
      "Audience": "Audience"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "x-Default": "Information",
      "x-Microsoft.AspNetCore": "Warning"
    }
  }
}
