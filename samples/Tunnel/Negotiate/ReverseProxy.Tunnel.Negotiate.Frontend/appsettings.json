{
  "Comment": "The tunnel front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "routeFERoot": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "routeFEBeta": {
        "ClusterId": "beta",
        "Match": {
          "Path": "/beta/{**catch-all}"
        }
      },
      "routeFEExample": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "Negotiate",
          "ClientCertificate": {
            "Path": "localhostclient1.pfx",
            "Password": "testPassword1"
          }
        }
      },
      "gamma": {
        "Destinations": {
          "beta/destination1": {
            "Address": "https://example.com/"
          }
        },
        "Transport": "Forwarder"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "LocalFile": {
      "LogLevel": {
        "Default": "Trace"
      },
      "FileName": "server1_"
    }
  }
}
