{
  "Comment": "The back end - inner front end",
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5003"
      }
    }
  },
  "ReverseProxy": {
    "Tunnels": {
      "tunnel5001alpha": {
        "Url": "https://localhost:5001",
        "RemoteTunnelId": "alpha",
        "Transport": "TunnelHTTP2",
        "Authentication": {
          "Mode": "Negotiate"
        }
      }
    },
    "Routes": {
      "routeBEroot": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Order": 101
      },
      "routeBEalpha": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "/alpha/{**catch-all}"
        },
        "Order": 100
      },
      "routeBEgamma": {
        "ClusterId": "gamma",
        "Match": {
          "Path": "/example"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha": {
            "Address": "https://localhost:5005"
          }
        }
      },
      "gamma": {
        "Destinations": {
          "gamma/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    },
    "AuthorizationTransport": {
      "SigningKeySecret": "testPassword-IsTooSmall!UseMoreText!elf!",
      "Algorithm": "http://www.w3.org/2001/04/xmldsig-more#hmac-sha256",
      "IncludeClaimType": ["ALL"],
      "EnableForAllCluster": true,
      "RemoveHeaderAuthenticate": true,
      "Scheme": "",
      "Issuer": "Issuer",
      "Audience": "Audience"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
